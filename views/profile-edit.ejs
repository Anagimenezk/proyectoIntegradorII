						 <input class="form-input" type="email" id="mail" placeholder="Correo electrónico" name="user_mail">
					 </li>
					 <li class="bordear">
						<!-- <label class="form-label" for="contraseña">Contraseña:</label>-->
						 <input class="form-input" type="password" id="contraseña"placeholder="Contraseña actual" name="user_contraseña">
						 <input class="form-input" type="password" id="nueva-contraseña"placeholder="Nueva contraseña" name="user_contraseña">
					 </li>
	
				
					<a class="actualizar-perfil" href="/profile">Actualizar mi perfil</a>
					
	
				   </ul>
